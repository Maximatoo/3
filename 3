using System;

class Program
{
    static void Main()
    {
        int result = AddTwoAndTwo();
        Console.WriteLine("2 + 2 = " + result);
    }

    static int AddTwoAndTwo()
    {
        string twoStr = new string('A', 2);
        string fourStr = twoStr + twoStr;

        int sum = 0;

        foreach (char ch in fourStr)
        {
            sum++;
        }

        return sum;
    }
}
